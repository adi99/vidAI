{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "expo start",
    "start": "expo start --reset-cache",
    "build:web": "expo export --platform web",
    "build:web:analyze": "ANALYZE_BUNDLE=true expo export --platform web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "deploy": "npx expo export -p web && npx eas-cli@latest deploy",
    "lint": "expo lint",
    "type-check": "tsc --noEmit",
    "clean": "expo start --clear",
    "prebuild": "expo prebuild --clean",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:android:production": "eas build --platform android --profile production",
    "build:ios:production": "eas build --platform ios --profile production",
    "optimize:images": "npx expo optimize",
    "analyze:bundle": "npx expo export --platform web && npx webpack-bundle-analyzer dist/static/js/*.js",
    "performance:audit": "npx lighthouse http://localhost:8081 --output html --output-path ./performance-audit.html",
    "cache:clear": "expo start --clear && npm run clean:cache",
    "clean:cache": "rm -rf node_modules/.cache && rm -rf .expo && rm -rf dist",
    "test": "echo \"No tests specified\" && exit 0",
    "test:performance": "node scripts/performance-test.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.51.0",
    "expo": "^53.0.20",
    "expo-application": "~6.1.5",
    "expo-auth-session": "^6.2.1",
    "expo-blur": "~14.1.3",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.3",
    "expo-crypto": "^14.1.5",
    "expo-device": "~7.1.4",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.3",
    "expo-iap": "^2.7.9",
    "expo-image-manipulator": "~13.1.3",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "~14.1.3",
    "expo-linking": "~7.1.3",
    "expo-localization": "~16.1.6",
    "expo-media-library": "^17.1.7",
    "expo-router": "~5.1.4",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.30.6",
    "expo-status-bar": "~2.2.2",
    "expo-symbols": "~0.4.3",
    "expo-system-ui": "~5.0.5",
    "expo-video": "~2.2.2",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.475.0",
    "moti": "^0.30.0",
    "onesignal-expo-plugin": "^2.0.3",
    "posthog-react-native": "^4.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-onesignal": "^5.2.13",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "jest": "^30.0.5",
    "jest-expo": "^53.0.9",
    "react-test-renderer": "^19.1.1",
    "typescript": "~5.8.3"
  }
}
