# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
SUPABASE_ANON_KEY=your_anon_key

# Server Configuration
PORT=3001
NODE_ENV=development

# JWT Configuration
JWT_SECRET=your_jwt_secret

# Upstash Redis (for queue system)
# Obtain from Upstash console. Example:
# UPSTASH_REDIS_URL=rediss://global-fancy-12345.upstash.io
# UPSTASH_REDIS_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
UPSTASH_REDIS_URL=your_upstash_redis_url
UPSTASH_REDIS_TOKEN=your_upstash_redis_token

# GPU Service Configuration
# Primary/fallback selection and timeouts
GPU_PRIMARY=modal
GPU_FALLBACK=runpod
GPU_TIMEOUT_MS=60000
GPU_RETRY_ATTEMPTS=2

# Modal.com
MODAL_API_KEY=your_modal_api_key
MODAL_BASE_URL=https://api.modal.com
# Paths to your deployed functions (adjust per your Modal setup)
MODAL_IMAGE_PATH=/v1/functions/image-generate
MODAL_VIDEO_PATH=/v1/functions/video-generate
MODAL_HEALTH_PATH=/v1/health

# Runpod.io
RUNPOD_API_KEY=your_runpod_api_key
RUNPOD_BASE_URL=https://api.runpod.ai
RUNPOD_IMAGE_ENDPOINT_ID=your_image_endpoint_id
RUNPOD_VIDEO_ENDPOINT_ID=your_video_endpoint_id
RUNPOD_POLL_INTERVAL_MS=3000
RUNPOD_HEALTH_PATH=/v2/health

# OpenRouter (image captioning)
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_CAPTION_MODEL=openai/gpt-4o-mini
OPENROUTER_SITE_URL=https://your-app-domain.com

# Push Notifications
EXPO_ACCESS_TOKEN=your_expo_access_token

# In-App Purchases
APPLE_SHARED_SECRET=your_apple_shared_secret
ANDROID_PACKAGE_NAME=com.yourapp.package

# Logging
LOG_LEVEL=info